[memory]
command=$HOME/nixos-config/dotfiles/i3blocks/scripts/memory
label=|  MEM: 
interval=30

[cpu_usage]
command=$HOME/nixos-config/dotfiles/i3blocks/scripts/cpu_usage
interval=10
LABEL=|  CPU: 

[disk]
command=$HOME/nixos-config/dotfiles/i3blocks/scripts/disk
LABEL=|  DISK: 
interval=30

[wifi]
command=$HOME/nixos-config/dotfiles/i3blocks/scripts/wifi
label=|  WIFI:
interval=60

[date]
command=date +"%Y-%m-%d"
label=|  DATE: 
interval=1

[time]
command=date +"%H:%M:%S"
label=|  TIME: 
interval=1

